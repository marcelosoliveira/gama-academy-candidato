    "host": "localhost:3000",
    "basePath": "/",
    "schemes": [
      "http"
    ],
    "paths": {
      "/candidato/buscar/{cpf}": {
        "get": {
          "summary": "Buscar candidato pelo CPF",
          "description": "Busca candidato pelo cpf",
          "operationId": "Buscar",
          "parameters": [{
            "name": "cpf",
            "in": "path",
            "description": "Insira um cpf válido.",
            "required": true,
            "type": "string"
          }],
          "responses": {}
        }
      },
      "/candidato/cadastrar": {
        "post": {
          "summary": "Cadastro de candidato",
          "description": "Cadastra curriculos dos candidatos para vagas de emprego.",
          "operationId": "Cadastro",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/candidato"
                },
                "example": {
                    "nome": "Jhonny Gallus",
                    "cpf": "333.444.555.34",
                    "cargo": "Desenvolvedor Java",
                    "email": "msbobsk8@hotmail.com",
                    "dataNasc": "08/10/1990",
                    "civil": "Solteiro",
                    "sexo": "Masculino",
                    "cep": "13950-000",
                    "endereco": "Rua Santa Ester, 130, casa",
                    "bairro": "São Leopoldo",
                    "cidade": "Lindóia",
                    "tel_fixo1": "1938982124",
                    "tel_fixo2": "1938981435",
                    "celular": "19998447295",
                    "contato": "19997654321",
                    "identidade": "428671239",
                    "possui_veiculo": "Sim",
                    "habilitacao": "AB"
                  }
              }
            }
          },
          "responses": {}
        }
      }
    },
    "components": {
      "schemas": {
        "candidato": {
          "type": "object",
          "properties": {
            "nome": {
              "type": "string"
            },
            "cpf": {
              "type": "string"
            },
            "cargo": {
